        "src/typings/process-0.11.x.d.ts",
        "src/typings/superagent-prefix-0.0.x.d.ts",
        "src/typings/validatorjs-2.0.x.d.ts",
        "src/api/RestAPI.ts",
        "src/api/WithAPI.ts",
        "src/config.ts",
        "src/errors/SDKError.ts",
        "src/index.ts",
        "src/resources/ApplicationTokens.ts",
        "src/resources/Authorization.ts",
        "src/resources/BankAccounts.ts",
        "src/resources/Charges.ts",
        "src/resources/common/ContactInfo.ts",
        "src/resources/common/GatewayCredentials.ts",
        "src/resources/CRUDResource.ts",
        "src/resources/Merchants.ts",
        "src/resources/Payouts.ts",
        "src/resources/Stores.ts",
        "src/resources/TransactionTokens.ts",
        "src/resources/Transfers.ts",
        "src/resources/WebHooks.ts",
        "src/utils/camelCase.ts",
        "src/utils/index.ts",
        "src/utils/underscore.ts"
    ],
    "exclude": [
        "node_modules"
    ]
