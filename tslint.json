{
  "extends": ["tslint:recommended"],
  "formatter": "tap",
  "rules": {
    "array-type": false,
    "interface-name": false,
    "max-line-length": [true, 140],
    "object-literal-sort-keys": false,
    "only-arrow-functions": false,
    "ordered-imports": false,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "semicolon": [true, "never"],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "no-console": false

  }
}
